-- Name: articles_article_id_seq; Type: SEQUENCE SET; Schema: public; Owner: cryptomob_user
--

SELECT pg_catalog.setval('public.articles_article_id_seq', 208, true);


--
-- Name: authors_author_id_seq; Type: SEQUENCE SET; Schema: public; Owner: cryptomob_user
--

SELECT pg_catalog.setval('public.authors_author_id_seq', 56, true);


--
-- Name: categories_category_id_seq; Type: SEQUENCE SET; Schema: public; Owner: cryptomob_user
--

SELECT pg_catalog.setval('public.categories_category_id_seq', 1, false);


--
-- Name: tags_tag_id_seq; Type: SEQUENCE SET; Schema: public; Owner: cryptomob_user
--

SELECT pg_catalog.setval('public.tags_tag_id_seq', 471, true);


--
-- Name: articles articles_article_link_key; Type: CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.articles
    ADD CONSTRAINT articles_article_link_key UNIQUE (article_link);


--
-- Name: articles articles_pkey; Type: CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.articles
    ADD CONSTRAINT articles_pkey PRIMARY KEY (article_id);


--
-- Name: articles_tags articles_tags_pk; Type: CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.articles_tags
    ADD CONSTRAINT articles_tags_pk PRIMARY KEY (article_id, tag_id);


--
-- Name: authors authors_author_name_key; Type: CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.authors
    ADD CONSTRAINT authors_author_name_key UNIQUE (author_name);


--
-- Name: authors authors_pkey; Type: CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.authors
    ADD CONSTRAINT authors_pkey PRIMARY KEY (author_id);


--
-- Name: categories categories_pkey; Type: CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.categories
    ADD CONSTRAINT categories_pkey PRIMARY KEY (category_id);


--
-- Name: tags tags_pkey; Type: CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.tags
    ADD CONSTRAINT tags_pkey PRIMARY KEY (tag_id);


--
-- Name: articles_tags articles_tags_article_id_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.articles_tags
    ADD CONSTRAINT articles_tags_article_id_fkey FOREIGN KEY (article_id) REFERENCES public.articles(article_id);


--
-- Name: articles_tags articles_tags_tag_id_fkey; Type: FK CONSTRAINT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.articles_tags
    ADD CONSTRAINT articles_tags_tag_id_fkey FOREIGN KEY (tag_id) REFERENCES public.tags(tag_id);


--
-- Name: articles fk_author; Type: FK CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.articles
    ADD CONSTRAINT fk_author FOREIGN KEY (author_id) REFERENCES public.authors(author_id);


--
-- Name: articles fk_category; Type: FK CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.articles
    ADD CONSTRAINT fk_category FOREIGN KEY (category_id) REFERENCES public.categories(category_id);


--
-- Name: articles fk_tag; Type: FK CONSTRAINT; Schema: public; Owner: cryptomob_user
--

ALTER TABLE ONLY public.articles
    ADD CONSTRAINT fk_tag FOREIGN KEY (tag_id) REFERENCES public.tags(tag_id);


--
-- Name: TABLE articles_tags; Type: ACL; Schema: public; Owner: postgres
--

GRANT ALL ON TABLE public.articles_tags TO cryptomob_user;


